<Window x:Class="WpfApp_Labs.Lab_3.Lab_3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp_Labs.Lab_3"
        mc:Ignorable="d"
        Title="Лабораторная работа №3" Height="275" Width="376">
    <Grid>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="Дано натуральное число n. Получить все его натуральные делители и записать в отдельный файл через диалог с пользователем." VerticalAlignment="Top" Width="348" Margin="10,10,0,0"/>
        <TextBox x:Name="TextBox" HorizontalAlignment="Left" Height="44" Margin="10,85,0,0" TextWrapping="Wrap" Text="Число" VerticalAlignment="Top" Width="348" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <Button Content="Вывести результат" HorizontalAlignment="Left" Margin="10,134,0,0" VerticalAlignment="Top" Width="348" Height="38" Click="Button_Click"/>
        <Label x:Name="Label" Content="Результат" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,177,0,0" Width="348" HorizontalContentAlignment="Center" Height="32"/>

    </Grid>
</Window>

<Window x:Class="WpfApp_Labs.Lab_3.Lab_3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp_Labs.Lab_3"
        mc:Ignorable="d"
        Title="Лабораторная работа №3" Height="275" MinWidth="380" SizeToContent="WidthAndHeight">
    <Grid>
        <TextBlock Background="AliceBlue" TextWrapping="Wrap" Text="Дано натуральное число n. Получить все его натуральные делители и записать в отдельный файл через диалог с пользователем." VerticalAlignment="Top" Width="348" Margin="10,10,10,0"/>
        <TextBox x:Name="TextBox" Height="44" Margin="10,85,10,0" TextWrapping="Wrap" Text="Число" Width="348" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" VerticalAlignment="Top"/>
        <Button Content="Вывести результат" Margin="10,134,10,0" Height="38" Click="Button_Click" VerticalAlignment="Top"/>
        <Label x:Name="Label" Content="Результат" Margin="10,177,10,0" HorizontalContentAlignment="Center" Height="32" VerticalAlignment="Top"/>

    </Grid>
</Window>
